{
    "root": "apps/services/comments",
    "sourceRoot": "apps/services/comments/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/node:webpack",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/services/comments",
                "main": "apps/services/comments/src/main.ts",
                "tsConfig": "apps/services/comments/tsconfig.app.json"
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "extractLicenses": true,
                    "inspect": false,
                    "fileReplacements": [{
                        "replace": "apps/services/comments/src/environments/environment.ts",
                        "with": "apps/services/comments/src/environments/environment.prod.ts"
                    }]
                }
            }
        },
        "serve": {
            "executor": "@nrwl/node:node",
            "options": {
                "buildTarget": "services-comments:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/services/comments/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/services/comments"],
            "options": {
                "jestConfig": "apps/services/comments/jest.config.js",
                "passWithNoTests": true
            }
        },
        "prisma-generate": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "prisma generate",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-dry": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma migrate dev --create-only --preview-feature --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-force": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma db push --force-reset --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-up": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma migrate dev --preview-feature --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-reset": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma migrate reset --preview-feature --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-deploy": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma migrate deploy --preview-feature --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "migrate-status": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma migrate status --preview-feature --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        },
        "studio": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [],
            "options": {
                "command": "../../../node_modules/.bin/prisma studio --schema ./prisma/schema.prisma",
                "cwd": "apps/services/comments"
            }
        }
    },
    "tags": []
}